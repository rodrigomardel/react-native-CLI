{
  "root": true,
  "extends": "@react-native",
  "rules": {
    "no-unused-vars": "off",
    "bracket-spacing": "off",
    "prettier/prettier": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": true,
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
